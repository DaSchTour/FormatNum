{
	"name": "mediawiki/FormatNum",
	"type": "mediawiki-extension",
	"description": "Extension FormatNum makes it easier to format numbers in the style you want, independent of language settings. For input 'english' format is used with point as decimal separator.",
	"keywords": [
		"Numbers",
		"Formating",
		"MediaWiki"
	],
	"homepage": "https://www.mediawiki.org/wiki/Extension:FormatNum",
	"license": "GPL-2.0+",
	"support": {
		"issues": "https://github.com/DaSchTour/FormatNum/issues",
		"source": "https://github.com/DaSchTour/FormatNum"
	},
	"authors": [
		{
            {
                    "name": "DaSch",
                    "email": "dasch@daschmedia.de",
                    "homepage": "http://www.daschmedia.de",
                    "role": "Developer"
            }
        ],
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1"
	},
	"autoload": {
		"files" : [
			"FormatNum.php"
		],
		"classmap": [
			"FormatNum.hooks.php"
		]
	}
}
